{
  "name": "relearn",
  "version": "2.0.0",
  "description": "This system reminds us of our favorite screenshots in Dropbox every day",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check --write src/",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "dropbox",
    "slack",
    "screenshot",
    "notification"
  ],
  "author": "danimal141",
  "license": "MIT",
  "dependencies": {
    "@slack/webhook": "^7.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
